use sysio as io;

fn main() {
	let a: int = 0d12;
	a = 1;
	io::println(a.to_str()); // 1

	let b: int = 0x0f;
	io::println(b.to_str()); // 15

	let c: int = 0b1010;
	io::println(c.to_str()); // 10

	let d: int = 0o12;
	io::println(d.to_str()); // 10

	let e: int = 0b1010_1010;
	io::println(e.to_str()); // 170
}